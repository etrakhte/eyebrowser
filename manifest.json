{
"manifest_version": 2,
    "name": "googly",
    "description": "turns all o's into googly eyes",
    "version": "1.0",
    "content_scripts": [
        {
            "matches": [
                "*://*/*"
            ],
            "js": [
                "content.js"
            ],
            "run_at": "document_end"
        }
    ]
}